{
    "name": "momo/simple-capture-tool",
    "description": "Capture tool using WebDriver",
    "type": "library",
    "license": "MIT",
    "require": {
        "ext-gd": "*",
        "adlawson/vfs": "^0.12.1",
        "facebook/webdriver": "^1.4",
        "league/csv": "^8.2",
        "symfony/config": "^2.8",
        "symfony/console": "^2.8",
        "symfony/yaml": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Momo\\SimpleCaptureTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "autoloader-suffix": "ABCDEFGHIJKLMNOPQRSTUVWXYZ",
        "platform": {
            "php": "5.5.8"
        },
        "preferred-install": {
            "*": "dist"
        },
        "secure-http": false,
        "sort-packages": true
    }

}
